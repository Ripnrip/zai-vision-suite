{
  "name": "zai-vision-suite",
  "version": "1.0.0",
  "description": "Z.ai Vision Suite - Multi-platform GLM-4.6V integration (Claude Code, Cursor, JetBrains, Sublime)",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "package:all": "pnpm run --filter '{\"packages/*\"}' --package-manager=pnpm",
    "postinstall": "echo 'ðŸ“¦ Z.ai Vision Suite installed! Run setup to configure your platforms.' && sleep 1"
  },
  "keywords": [
    "vision",
    "image",
    "video",
    "GLM",
    "Z.ai",
    "multimodal",
    "OCR",
    "claude",
    "cursor",
    "jetbrains"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.19.11",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "dotenv": "^16.6.1",
    "eslint": "^9.0.0",
    "typescript": "^5.9.3",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "exports": {
    ".": {
      "types": "./src/index.d.ts"
    },
    "./packages/shared": {
      "types": "./src/shared/index.d.ts"
    },
    "./packages/claude-code": {
      "types": "./packages/claude-code/dist/index.d.ts",
      "import": "./packages/claude-code/dist/index.js"
    }
  }
}
